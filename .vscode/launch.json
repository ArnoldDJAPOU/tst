{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Localserver",
            "request": "launch",
            "type": "al",
            "environmentType": "OnPrem",
            "server": "http://localhost:8081/TST",
            "serverInstance": "TST",
            "authentication": "Windows",
            "startupObjectId": 22,
            "startupObjectType": "Page",
